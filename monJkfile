pipeline {
    agent any 
        stages {
          stage('Build') {
            steps {
                echo "etape1"
                sh 'tree /tmp'
                echo "${env.BUILD_NUMBER}"
            }
          }
          stage('Envtest') {
            environment {
                LANG= 'C'
            }
            steps {
                echo "test with env C"
                sh 'tree /tmp'
                echo "${env.BUILD_NUMBER}"
            }
          }
        }
      }
