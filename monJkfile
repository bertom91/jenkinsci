pipeline {
    agent any 
    stages {
        stage('Stage 1') {
            steps {
                echo 'Hello from github' 
            }
        }
        stage('Vérification') {
            steps {
                echo "Vérif stage"
            }
        }
        stage('Deploy') {
    steps {
        echo "Deploy stage"
    }
}  
    }
}
